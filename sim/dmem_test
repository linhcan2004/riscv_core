#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2009.vpi";
S_0x5f1d8f6f84f0 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x5f1d8f6f8680 .scope module, "dmem_tb" "dmem_tb" 3 3;
 .timescale -9 -12;
v0x5f1d8f70f840_0 .var "addr_tb", 31 0;
v0x5f1d8f70f920_0 .var "clk", 0 0;
v0x5f1d8f70f9f0_0 .net "rdata_tb", 31 0, L_0x5f1d8f6e70e0;  1 drivers
v0x5f1d8f70faf0_0 .var "wdata_tb", 31 0;
v0x5f1d8f70fbc0_0 .var "we_tb", 0 0;
S_0x5f1d8f6bd490 .scope module, "dut" "dmem" 3 10, 4 1 0, S_0x5f1d8f6f8680;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "we";
    .port_info 2 /INPUT 32 "addr";
    .port_info 3 /INPUT 32 "wdata";
    .port_info 4 /OUTPUT 32 "rdata";
L_0x5f1d8f6e70e0 .functor BUFZ 32, L_0x5f1d8f70fcb0, C4<00000000000000000000000000000000>, C4<00000000000000000000000000000000>, C4<00000000000000000000000000000000>;
v0x5f1d8f6e7240 .array "RAM", 63 0, 31 0;
v0x5f1d8f6e7750_0 .net *"_ivl_0", 31 0, L_0x5f1d8f70fcb0;  1 drivers
v0x5f1d8f70f270_0 .net *"_ivl_3", 29 0, L_0x5f1d8f70fdb0;  1 drivers
v0x5f1d8f70f330_0 .net "addr", 31 0, v0x5f1d8f70f840_0;  1 drivers
v0x5f1d8f70f410_0 .net "clk", 0 0, v0x5f1d8f70f920_0;  1 drivers
v0x5f1d8f70f520_0 .net "rdata", 31 0, L_0x5f1d8f6e70e0;  alias, 1 drivers
v0x5f1d8f70f600_0 .net "wdata", 31 0, v0x5f1d8f70faf0_0;  1 drivers
v0x5f1d8f70f6e0_0 .net "we", 0 0, v0x5f1d8f70fbc0_0;  1 drivers
E_0x5f1d8f6bd910 .event posedge, v0x5f1d8f70f410_0;
L_0x5f1d8f70fcb0 .array/port v0x5f1d8f6e7240, L_0x5f1d8f70fdb0;
L_0x5f1d8f70fdb0 .part v0x5f1d8f70f840_0, 2, 30;
    .scope S_0x5f1d8f6bd490;
T_0 ;
    %wait E_0x5f1d8f6bd910;
    %load/vec4 v0x5f1d8f70f6e0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %load/vec4 v0x5f1d8f70f600_0;
    %load/vec4 v0x5f1d8f70f330_0;
    %parti/s 30, 2, 3;
    %ix/vec4 3;
    %ix/load 4, 0, 0; Constant delay
    %assign/vec4/a/d v0x5f1d8f6e7240, 0, 4;
T_0.0 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5f1d8f6f8680;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5f1d8f70f920_0, 0, 1;
T_1.0 ;
    %delay 5000, 0;
    %load/vec4 v0x5f1d8f70f920_0;
    %inv;
    %store/vec4 v0x5f1d8f70f920_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x5f1d8f6f8680;
T_2 ;
    %vpi_call/w 3 24 "$dumpfile", "sim/dmem_wave.vcd" {0 0 0};
    %vpi_call/w 3 25 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5f1d8f6f8680 {0 0 0};
    %vpi_call/w 3 27 "$monitor", "Time=%0t | WE=%b | Addr=%d | Wdata=%h | Rdata=%h", $time, v0x5f1d8f70fbc0_0, v0x5f1d8f70f840_0, v0x5f1d8f70faf0_0, v0x5f1d8f70f9f0_0 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5f1d8f70fbc0_0, 0, 1;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5f1d8f70f840_0, 0, 32;
    %pushi/vec4 2916003789, 0, 32;
    %store/vec4 v0x5f1d8f70faf0_0, 0, 32;
    %wait E_0x5f1d8f6bd910;
    %delay 10000, 0;
    %pushi/vec4 4, 0, 32;
    %store/vec4 v0x5f1d8f70f840_0, 0, 32;
    %pushi/vec4 3738168538, 0, 32;
    %store/vec4 v0x5f1d8f70faf0_0, 0, 32;
    %wait E_0x5f1d8f6bd910;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5f1d8f70fbc0_0, 0, 1;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5f1d8f70f840_0, 0, 32;
    %delay 10000, 0;
    %pushi/vec4 4, 0, 32;
    %store/vec4 v0x5f1d8f70f840_0, 0, 32;
    %delay 10000, 0;
    %vpi_call/w 3 46 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
