#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2009.vpi";
S_0x5b8012e803f0 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x5b8012e80580 .scope module, "imem_tb" "imem_tb" 3 3;
 .timescale -9 -12;
v0x5b8012e93d10_0 .var "addr_tb", 31 0;
v0x5b8012e93dd0_0 .net "data_tb", 31 0, L_0x5b8012e940e0;  1 drivers
S_0x5b8012e46490 .scope module, "dut" "imem" 3 7, 4 1 0, S_0x5b8012e80580;
 .timescale 0 0;
    .port_info 0 /INPUT 32 "addr";
    .port_info 1 /OUTPUT 32 "data";
L_0x5b8012e940e0 .functor BUFZ 32, L_0x5b8012e93ea0, C4<00000000000000000000000000000000>, C4<00000000000000000000000000000000>, C4<00000000000000000000000000000000>;
v0x5b8012e46670 .array "RAM", 63 0, 31 0;
v0x5b8012e46710_0 .net *"_ivl_0", 31 0, L_0x5b8012e93ea0;  1 drivers
v0x5b8012e939e0_0 .net *"_ivl_3", 29 0, L_0x5b8012e93fa0;  1 drivers
v0x5b8012e93aa0_0 .net "addr", 31 0, v0x5b8012e93d10_0;  1 drivers
v0x5b8012e93b80_0 .net "data", 31 0, L_0x5b8012e940e0;  alias, 1 drivers
L_0x5b8012e93ea0 .array/port v0x5b8012e46670, L_0x5b8012e93fa0;
L_0x5b8012e93fa0 .part v0x5b8012e93d10_0, 2, 30;
    .scope S_0x5b8012e46490;
T_0 ;
    %vpi_call/w 4 11 "$readmemh", "program.hex", v0x5b8012e46670 {0 0 0};
    %end;
    .thread T_0;
    .scope S_0x5b8012e80580;
T_1 ;
    %vpi_call/w 3 13 "$dumpfile", "sim/imem_wave.vcd" {0 0 0};
    %vpi_call/w 3 14 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5b8012e80580 {0 0 0};
    %vpi_call/w 3 16 "$monitor", "Time=%0t | PC_addr=%d | Instruction=%h", $time, v0x5b8012e93d10_0, v0x5b8012e93dd0_0 {0 0 0};
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5b8012e93d10_0, 0, 32;
    %delay 10000, 0;
    %pushi/vec4 4, 0, 32;
    %store/vec4 v0x5b8012e93d10_0, 0, 32;
    %delay 10000, 0;
    %pushi/vec4 8, 0, 32;
    %store/vec4 v0x5b8012e93d10_0, 0, 32;
    %delay 10000, 0;
    %pushi/vec4 12, 0, 32;
    %store/vec4 v0x5b8012e93d10_0, 0, 32;
    %delay 10000, 0;
    %vpi_call/w 3 29 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 5;
    "N/A";
    "<interactive>";
    "-";
    "tb/imem_tb.sv";
    "rtl/imem.sv";
