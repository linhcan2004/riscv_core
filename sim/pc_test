#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2009.vpi";
S_0x5e4e88e5ac10 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x5e4e88e5ada0 .scope module, "pc_tb" "pc_tb" 3 3;
 .timescale -9 -12;
v0x5e4e88e70160_0 .var "clk", 0 0;
v0x5e4e88e70220_0 .var "pc_next_tb", 31 0;
v0x5e4e88e702f0_0 .net "pc_out_tb", 31 0, v0x5e4e88e6ff60_0;  1 drivers
v0x5e4e88e703f0_0 .var "rst_n_tb", 0 0;
E_0x5e4e88e21000 .event posedge, v0x5e4e88e49010_0;
S_0x5e4e88e20490 .scope module, "dut" "pc" 3 9, 4 1 0, S_0x5e4e88e5ada0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst_n";
    .port_info 2 /INPUT 32 "pc_next";
    .port_info 3 /OUTPUT 32 "pc_out";
v0x5e4e88e49010_0 .net "clk", 0 0, v0x5e4e88e70160_0;  1 drivers
v0x5e4e88e493b0_0 .net "pc_next", 31 0, v0x5e4e88e70220_0;  1 drivers
v0x5e4e88e6ff60_0 .var "pc_out", 31 0;
v0x5e4e88e70020_0 .net "rst_n", 0 0, v0x5e4e88e703f0_0;  1 drivers
E_0x5e4e88e59b00/0 .event negedge, v0x5e4e88e70020_0;
E_0x5e4e88e59b00/1 .event posedge, v0x5e4e88e49010_0;
E_0x5e4e88e59b00 .event/or E_0x5e4e88e59b00/0, E_0x5e4e88e59b00/1;
    .scope S_0x5e4e88e20490;
T_0 ;
    %wait E_0x5e4e88e59b00;
    %load/vec4 v0x5e4e88e70020_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x5e4e88e6ff60_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5e4e88e493b0_0;
    %assign/vec4 v0x5e4e88e6ff60_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5e4e88e5ada0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5e4e88e70160_0, 0, 1;
T_1.0 ;
    %delay 5000, 0;
    %load/vec4 v0x5e4e88e70160_0;
    %inv;
    %store/vec4 v0x5e4e88e70160_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x5e4e88e5ada0;
T_2 ;
    %vpi_call/w 3 22 "$dumpfile", "sim/pc_wave.vcd" {0 0 0};
    %vpi_call/w 3 23 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5e4e88e5ada0 {0 0 0};
    %vpi_call/w 3 25 "$monitor", "Time=%0t | Reset=%b | PC_next=%d | PC_out=%d", $time, v0x5e4e88e703f0_0, v0x5e4e88e70220_0, v0x5e4e88e702f0_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5e4e88e703f0_0, 0, 1;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5e4e88e70220_0, 0, 32;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5e4e88e703f0_0, 0, 1;
    %wait E_0x5e4e88e21000;
    %pushi/vec4 4, 0, 32;
    %store/vec4 v0x5e4e88e70220_0, 0, 32;
    %delay 10000, 0;
    %wait E_0x5e4e88e21000;
    %pushi/vec4 8, 0, 32;
    %store/vec4 v0x5e4e88e70220_0, 0, 32;
    %delay 10000, 0;
    %pushi/vec4 100, 0, 32;
    %store/vec4 v0x5e4e88e70220_0, 0, 32;
    %wait E_0x5e4e88e21000;
    %delay 10000, 0;
    %vpi_call/w 3 42 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 5;
    "N/A";
    "<interactive>";
    "-";
    "tb/pc_tb.sv";
    "rtl/pc.sv";
